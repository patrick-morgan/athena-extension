{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Athena AI Extension",
  "description": "Find the truth",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "Athena AI"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentScript.js"]
    }
  ]
}
