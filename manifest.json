{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Athena AI Extension",
  "description": "Find the truth",
  "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----",
  "action": {
    "default_popup": "js/index.html",
    "default_icon": {
      "16": "owl-logo-png.png",
      "48": "owl-logo-png.png",
      "128": "owl-logo-png.png"
    },
    "default_title": "Athena AI"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "tabs",
    "identity",
    "https://peoplespress.news/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "externally_connectable": {
    "matches": ["https://peoplespress.news/*"]
  },
  "host_permissions": ["https://*.firebaseapp.com/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "oauth2": {
    "client_id": "981341455175-uaf4bg5bl7voulhnmsvbp312d2bot18i.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentScript.js"]
    }
  ]
}
