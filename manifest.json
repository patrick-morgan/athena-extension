{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Athena AI Extension",
  "description": "Find the truth",
  "key": "jofbailocnoaakmplpmifackkpcpckpp",
  "action": {
    "default_popup": "js/index.html",
    "default_icon": {
      "16": "owl-logo-png.png",
      "48": "owl-logo-png.png",
      "128": "owl-logo-png.png"
